create table student(id int primary key,name char(10),cgpa real,height real);
insert into student values(101,"Bhavana",8.9,5), (102,"Manasa",9,5.4), (103,"srija",8.99,5.9);
create view info as select * from student;
select * from info;
insert into info values(104,"spoorthi",6,8.8);
select name as Student_Name from info where height>5.5;
update info set height=5.2 where id=101;
delete from info where height=5.9;
select * from info;
