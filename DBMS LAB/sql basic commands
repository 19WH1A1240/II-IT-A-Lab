create database 19WH1A1240;
use 19WH1A1240;
show tables;
create table tab1(hno int primary key, name varchar(100), cgpa real);
show tables;
insert into tab1 values(1240,"Bhavana",9);
insert into tab1 values(1241,"Bhavana",8.9);
insert into tab1 values(1242,"srija",7.8);
insert into tab1 values(1243,"spoorthi",10);
insert into tab1 values(1244,"shivani",9);
update tab1 set cgpa=8.8 where hno=1243;
update tab1 set name="sreeja" where hno=1242;
select * from tab1;
alter table tab1 add age int;
update tab1 set age=18;
select * from tab1;
alter table tab1 modify cgpa int;
select * from tab1;
alter table tab1 change cgpa total_cgpa char;
select * from tab1;
select DISTINCT name from tab1;
